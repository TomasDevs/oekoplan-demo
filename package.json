{
  "name": "oekoplan",
  "version": "1.0.0",
  "description": "Demo Oekoplan Tailwind CSS",
  "scripts": {
    "build": "tailwindcss build src/styles.css -o public/css/styles.css && webpack --mode production",
    "watch": "tailwindcss -i ./src/styles.css -o ./public/css/styles.css --watch",
    "webpack:build": "webpack --mode production",
    "webpack:watch": "webpack --watch"
  },
  "devDependencies": {
    "@babel/core": "^7.26.0",
    "@babel/preset-env": "^7.26.0",
    "babel-loader": "^9.2.1",
    "tailwindcss": "^3.4.14",
    "webpack": "^5.96.1",
    "webpack-cli": "^5.1.4"
  },
  "dependencies": {
    "alpinejs": "^3.14.3",
    "gsap": "^3.12.5"
  }
}
